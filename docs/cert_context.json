{
  "@context": {
    "schema": "https://schema.org/",
    "gs1": "https://gs1.org/voc/",
    "gs1x": "https://gs1.org/voc/x/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",

    "CertificationReport": "gs1x:CertificationReport",
    "Product": "gs1:Product",
    "CertificationDetails": "gs1:CertificationDetails",
    "Country": "gs1:Country",

    "issueDate": {
      "@id": "gs1x:issueDate",
      "@type": "xsd:dateTime"
    },
    "issuer": {
      "@id": "gs1x:issuer",
      "@type": "schema:Text"
    },
    "products": {
      "@id": "gs1x:products"
    },

    "gtin": {
      "@id": "gs1:gtin",
      "@type": "gs1:gtin"
    },
    "description": {
      "@id": "gs1:productDescription",
      "@type": "schema:Text"
    },
    "certificationInfo": {
      "@id": "gs1:certificationDetails",
      "@container": "@set"
    },

    "agency": "gs1:certificationAgency",
    "endDate": {
      "@id": "gs1:certificationEndDate",
      "@type": "schema:Date"
    },
    "standard": "gs1:certificationStandard",
    "status": "gs1:certificationStatus",
    "value": "gs1:certificationValue",

    "targetCountries": {
      "@id": "gs1:targetCountry",
      "@type": "@id"
    },
    "countryCode": "schema:countryCode"
  }
}
